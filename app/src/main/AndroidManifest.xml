<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.behealthy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.FoodFeedActivity"></activity>
        <activity android:name=".activity.FoodActivity" />
        <activity android:name=".activity.VegetableDetailActivity" />
        <activity android:name=".activity.VitaminsActivity" />
        <activity android:name=".activity.VitaminListActivity" />
        <activity android:name=".activity.DailyFeedActivity" />
        <activity android:name=".activity.RfmActivity" />
        <activity android:name=".activity.BmiActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>